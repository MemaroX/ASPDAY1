@model List<ASPDAY1.Models.Student>

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Student List</title>
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" rel="stylesheet" />
</head>
<body>
    <div class="container">
        <h2 class="mt-4">Student List</h2>
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Age</th>
                    <th>Email</th>
                    <th>Phone Number</th>
                    <th>Address</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var student in Model)
                {
                    <tr>
                        <td>@student.Id</td>
                        <td>@student.Name</td>
                        <td>@student.Age</td>
                        <td>@student.Email</td>
                        <td>@student.PhoneNumber</td>
                        <td>@student.Address</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>
